{
    "extends": "ts-node/node16/tsconfig.json",
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "lib": [
        "esnext",
        "dom"
      ],
      "strictPropertyInitialization": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "noImplicitAny": false
    },
    "exclude": ["node_modules"]
  }